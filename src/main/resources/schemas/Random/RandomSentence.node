# Random.node
description: "Generates a random number within a specified range"
group: Random
name: Random Word
extends: Base/Node

theme:
  color: '#213752'
  width: 140
  height: 40
  icon: 0xe55f
edges:
  result:
    description: 'The generated random word'
    direction: output
    type: string
source: |
  local _Random = java.import('noderspace.common.utils.Random')
  local function ${NODE.name}_${NODE.uid}()
    return _Random:randomSentence()
  end
