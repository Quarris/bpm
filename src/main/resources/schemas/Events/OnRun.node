# OnRun.node
description: "The main event node for a workflow run."
group: Events
name: Run
extends: Base/Node
theme:
  color: '#f46ee0'
  width: 69
  height: 40
  icon: 0xf471
edges:
  exec_out:
    description: 'The output execution flow'
    direction: output
    type: exec

source: |
  local function ${NODE.name}_${NODE.uid}()
    ${EXEC.exec_out}
  end